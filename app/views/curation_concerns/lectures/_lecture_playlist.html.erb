<audio controls="controls" class="hls-media" preload="auto">
    <% file_set_ids.each do |file_set_id| %>
        <source src="<%= File.join(request.base_url,"file_set",file_set_id,"hls.m3u8") %>" type="application/x-mpegurl"/>
    <%end %>
        Your browser does not support the audio tag.
</audio>
<script type="text/javascript">isPlaylist = <%= file_set_ids.count > 1 ? "true" : "false" %></script>
