<% ScoobySnacks::METADATA_SCHEMA.secondary_display_fields.each do |field| %>
    <% Rails.logger.warn "displaying metadata field: #{field.name}" %>
    <% Rails.logger.warn "with display options: #{field.display_options}" %>
    <%= presenter.attribute_to_html(field.name.to_sym, field.display_options) %>
    <% next if (field.name == "dateCreated") && !presenter.attribute_to_html(:dateCreatedDisplay, label: "Date Created").blank? %>
<% end %>
