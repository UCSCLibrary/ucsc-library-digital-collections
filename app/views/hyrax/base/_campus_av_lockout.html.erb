<%#= Custom file %> 

<div id="primary-audio-div" >

  <div id="off-campus-access-notice" class="audio-player-substitute">
    <p>You must be on the UC Santa Cruz campus or logged into campus VPN to access this item.</p>
    <a href="https://guides.library.ucsc.edu/cabrillo-music-festival" class="btn btn-primary">Learn More</a>
  </div>
    <% if av_files.count > 1 %>
        <div id="track-list">
            <p>Track List</p>
            <% av_files.each do |av_file| %>
                <% if av_file[:playing] %>
                    <span class="glyphicon glyphicon-stats" aria-hidden="true"></span>
                <% end %>
                <div class="av_playlist_link <%= "active" if av_file[:playing] %>" id="div-<%= av_file[:id] %>">
                    <% if av_file[:playing] && av_file[:link].include?(@presenter.id) %>
                        <%= av_file[:title] %>
                    <% else %>
                        <%= link_to av_file[:title], av_file[:link] %>
                    <% end %>
                </div>
            <% end %>
        </div>
    <% end %>
</div>

<%= javascript_include_tag "include_player" %>
<%= stylesheet_link_tag "embed" %>


