<%= presenter.attribute_to_html(:titleAlternative, label: "Alternative Title:") %>
<%= presenter.attribute_to_html(:subseries, label: "SubSeries Title:") %>
<%= presenter.attribute_to_html(:description, label: "Description:") %>
<%= presenter.attribute_to_html(:series, render_as: :faceted, label: "Series") %>
<%= presenter.attribute_to_html(:creator_label, render_as: :faceted, label: "Creator") %>
<%= presenter.attribute_to_html(:subject_label, render_as: :faceted, label: "Subject") %>
<%= presenter.attribute_to_html(:subjectTopic_label, render_as: :faceted, label: "Subject") %>
<%= presenter.attribute_to_html(:subjectName_label, render_as: :faceted, label: "Subject (Person)") %>
<%= presenter.attribute_to_html(:subjectPlace_label, render_as: :faceted, label: "Subject (Place)") %>
<%= presenter.attribute_to_html(:theme, render_as: :faceted, label: "Theme") %>
<%= presenter.attribute_to_html(:publisher, label: "Publisher") %>
<%= presenter.attribute_to_html(:resourceType_label, label: "Resource Type") %>
<%= presenter.attribute_to_html(:keyword, render_as: :faceted) %>
<% unless (display_date = presenter.attribute_to_html(:dateCreatedDisplay, label: "Date Created")).blank? %>
    <%= display_date %>
<% else %>
    <%= presenter.attribute_to_html(:dateCreated, label: "Date Created")  %>
<% end %>


