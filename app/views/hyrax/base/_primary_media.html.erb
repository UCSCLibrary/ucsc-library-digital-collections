<% if av_files.any? %>
    <% if av_files.count == 1 %>
        <%= render 'single_player', file_set_id: av_files.first  %>
    <% else %>
        <%= render 'playlist', file_set_ids: av_files  %>
    <% end %>
    <%= javascript_include_tag "include_player" %>
    <%= stylesheet_link_tag "embed" %>
<% elsif @presenter.representative_id && @presenter.solr_document.image? || @presenter.solr_document.resource_type.include?("http://purl.org/dc/dcmitype/StillImage")%>
    <%= image_tag @presenter.solr_document.display_image_path, class: "primary-media",role: "presentation"%>
<% end %>
