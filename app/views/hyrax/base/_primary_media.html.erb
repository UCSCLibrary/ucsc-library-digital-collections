<% if av_files.any? %>
    <% if av_files.count == 1 %>
        <%= render 'single_player', file_set_id: av_files.first  %>
    <% else %>
        <%= render 'playlist', file_set_ids: av_files  %>
    <% end %>
    <%= javascript_include_tag "include_player" %>
    <%= stylesheet_link_tag "embed" %>
<% elsif @presenter.representative_id %>
    <%= image_tag @presenter.solr_document.display_image_url, class: "primary-media",role: "presentation"%>
<% end %>
